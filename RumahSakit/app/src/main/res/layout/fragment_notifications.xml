<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".janji">

    <LinearLayout
        android:layout_width="356dp"
        android:layout_height="45dp"
        android:orientation="horizontal"
        android:paddingLeft="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.05">

        <EditText
            android:id="@+id/carilah"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="2dp"
            android:background="@drawable/bg_wht_round"
            android:hint="Nama atau Hari atau Spesialis" />

        <Button
            android:id="@+id/btn_src"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/colorPrimary"
            android:text="Cari"
            android:textColor="@color/white" />
    </LinearLayout>

    <TextView
        android:id="@+id/kotakjanji"
        android:layout_width="337dp"
        android:layout_height="317dp"
        android:background="@drawable/kotakjanji"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.59000003" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="135dp"
        android:background="@drawable/et_bg"
        android:text="Janji Anda"
        android:textColor="@color/colorSecond"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.47000003" />

    <LinearLayout
        android:layout_width="290dp"
        android:layout_height="50dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.48000002">

        <TextView
            android:layout_width="132dp"
            android:layout_height="match_parent"
            android:paddingTop="16dp"
            android:text="Hari"
            android:textSize="22sp" />

        <TextView
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:paddingTop="16dp"
            android:text=":"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/doc_hari"
            android:layout_width="148dp"
            android:layout_height="match_parent"
            android:paddingTop="16dp"
            android:text="Hari ini"
            android:textSize="22sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="290dp"
        android:layout_height="45dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.56">

        <TextView
            android:layout_width="132dp"
            android:layout_height="match_parent"
            android:paddingTop="16dp"
            android:text="No. Antrian"
            android:textSize="22sp" />

        <TextView
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:paddingTop="16dp"
            android:text=":"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/profemail"
            android:layout_width="148dp"
            android:layout_height="match_parent"
            android:paddingTop="16dp"
            android:text="999"
            android:textSize="22sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="290dp"
        android:layout_height="42dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.63">

        <TextView
            android:layout_width="132dp"
            android:layout_height="match_parent"
            android:paddingTop="16dp"
            android:text="Nama Dokter"
            android:textSize="22sp" />

        <TextView
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:paddingTop="16dp"
            android:text=":"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/doc_nama"
            android:layout_width="148dp"
            android:layout_height="match_parent"
            android:paddingTop="16dp"
            android:text="dr X"
            android:textSize="22sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="290dp"
        android:layout_height="46dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.7">

        <TextView
            android:layout_width="132dp"
            android:layout_height="match_parent"
            android:paddingTop="16dp"
            android:text="Spesialis"
            android:textSize="22sp" />

        <TextView
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:paddingTop="16dp"
            android:text=":"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/doc_spesial"
            android:layout_width="148dp"
            android:layout_height="match_parent"
            android:paddingTop="16dp"
            android:text="Sp. X"
            android:textSize="22sp" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>